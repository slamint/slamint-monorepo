{
  "name": "@slamint/core",
  "version": "0.0.1",
  "private": true,
  "type": "commonjs",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "@slamint/source": "./src/index.ts",
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "peerDependencies": {
    "@nestjs/common": "^11.0.0",
    "@nestjs/config": "^4.0.0",
    "@nestjs/microservices": "^11.0.0",
    "@nestjs/typeorm": "^11.0.0",
    "@nestjs/swagger": "^11.0.0",
    "typeorm": "^0.3.0",
    "rxjs": "^7.8.0",
    "dotenv": "^17.2.2",
    "zod": "^4.1.8",
    "express": "5.1.0",
    "tslib": "^2.8.1"
  },
  "devDependencies": {
    "@nestjs/common": "^11.0.0",
    "@nestjs/core": "^11.0.0",
    "@nestjs/config": "^4.0.0",
    "@nestjs/microservices": "^11.0.0",
    "@nestjs/typeorm": "^11.0.0",
    "@nestjs/swagger": "^11.0.0",
    "typeorm": "^0.3.26",
    "rxjs": "^7.8.2",
    "@types/express": "^5.0.3",
    "tslib": "^2.8.1"
  },
  "nx": {
    "targets": {
      "build": {
        "executor": "@nx/js:tsc",
        "options": {
          "outputPath": "common/core/dist",
          "tsConfig": "common/core/tsconfig.lib.json",
          "packageJson": "common/core/package.json",
          "main": "common/core/src/index.ts",
          "assets": [
            "common/core/*.md"
          ]
        }
      }
    }
  }
}
