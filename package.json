{
  "name": "@slamint/source",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start:dev": "nx run-many --target=serve --all --configuration=development --output-style=stream --verbose",
    "build": "nx run-many --target=build --all  --output-style=stream",
    "docker:build": "nx run-many --target=docker-build --projects=@slamint/api-gateway,@slamint/account-management --output-style=stream",
    "docker:release": "nx run-many --target=docker-release --projects=@slamint/api-gateway,@slamint/account-management --output-style=stream",
    "accmgmt:migration:create": "cd api/account-management && ../../node_modules/.bin/typeorm migration:create src/migrations/$NAME",
    "accmgmt:migration:gen": "cd api/account-management && ts-node -r tsconfig-paths/register ../../node_modules/typeorm/cli migration:generate src/migrations/$NAME --pretty -d src/data-source.ts",
    "accmgmt:migration:run": "cd api/account-management && ts-node -r tsconfig-paths/register ../../node_modules/typeorm/cli migration:run -d src/data-source.ts",
    "accmgmt:migration:revert": "cd api/account-management && ts-node -r tsconfig-paths/register ../../node_modules/typeorm/cli migration:revert -d src/data-source.ts"
  },
  "private": true,
  "dependencies": {
    "@nestjs/common": "^11.0.0",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.0",
    "@nestjs/microservices": "^11.1.6",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.6",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/typeorm": "^11.0.0",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/semantic-conventions": "^1.37.0",
    "axios": "^1.12.1",
    "cache-manager": "^7.2.2",
    "cacheable": "^2.0.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "compression": "^1.8.1",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "jwks-rsa": "^3.2.0",
    "keyv": "^5.5.2",
    "on-finished": "^2.4.1",
    "passport-jwt": "^4.0.1",
    "pg": "^8.16.3",
    "pino": "^9.9.5",
    "pino-http": "^10.5.0",
    "pino-std-serializers": "^7.0.0",
    "reflect-metadata": "^0.1.14",
    "rxjs": "^7.8.0",
    "typeorm": "^0.3.26",
    "zod": "^4.1.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.8.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.0",
    "@nx/docker": "21.5.2",
    "@nx/esbuild": "21.5.2",
    "@nx/eslint": "21.5.2",
    "@nx/eslint-plugin": "21.5.2",
    "@nx/js": "21.5.2",
    "@nx/nest": "21.5.2",
    "@nx/node": "21.5.2",
    "@nx/web": "21.5.2",
    "@nx/webpack": "21.5.2",
    "@nx/workspace": "21.5.2",
    "@swc-node/register": "~1.9.1",
    "@swc/core": "~1.13.3",
    "@swc/helpers": "~0.5.11",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "20.19.9",
    "@types/on-finished": "^2.3.5",
    "@types/passport-jwt": "^4.0.1",
    "dotenv": "^17.2.2",
    "esbuild": "^0.19.2",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^10.0.0",
    "jsonc-eslint-parser": "^2.1.0",
    "nx": "21.5.2",
    "pino-pretty": "^13.1.1",
    "prettier": "^2.6.2",
    "prettier-plugin-organize-imports": "^4.3.0",
    "source-map-loader": "^5.0.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths-webpack-plugin": "^4.2.0",
    "tslib": "^2.8.1",
    "typescript": "~5.9.2",
    "typescript-eslint": "^8.40.0",
    "verdaccio": "^6.0.5",
    "webpack-cli": "^5.1.4"
  },
  "nx": {
    "includedScripts": [],
    "targets": {
      "local-registry": {
        "executor": "@nx/js:verdaccio",
        "options": {
          "port": 4873,
          "config": ".verdaccio/config.yml",
          "storage": "tmp/local-registry/storage"
        }
      }
    }
  }
}
